cont = 0
cont1 =0
from datetime import date
atual = date.today().year
for i in range(1,8):
    x = int(input('Em que ano a {}  nasceu?'.format(i)))
    y = atual - x
    if y>= 21:
        cont += 1
    else:
        cont1 += 1
print('HÃ¡ ',cont,'pessoas maiores de 21 e',cont1,'pessoas com menos de 21')
